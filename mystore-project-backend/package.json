{
  "name": "mystore-backend",
  "version": "0.1.0",
  "description": "MyStore Backend API",
  "main": "server.ts",
  "scripts": {
    "db-setup": "db-migrate up",
    "db-teardown": "db-migrate down",
    "start": "node dist/server",
    "formatter": "npm run prettier && npm run lint",
    "lint": "eslint --config .eslintrc 'src/**/*.ts'",
    "lint-and-fix": "eslint --config .eslintrc 'src/**/*.ts' --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "build": "tsc",
    "test": "export ENV='test' && tsc && db-migrate --env test up && jasmine-ts && db-migrate reset --env test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/parser": "^4.31.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "tslog": "^3.2.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.9.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.4.0",
    "supertest": "^6.1.6",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
